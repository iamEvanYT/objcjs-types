{
  "name": "objcjs-types",
  "version": "0.2.1",
  "description": "Auto-generated TypeScript type declarations for macOS Objective-C frameworks",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./delegates": {
      "types": "./dist/delegates.d.ts",
      "default": "./dist/delegates.js"
    },
    "./helpers": {
      "types": "./dist/helpers.d.ts",
      "default": "./dist/helpers.js"
    },
    "./nsdata": {
      "types": "./dist/nsdata.d.ts",
      "default": "./dist/nsdata.js"
    },
    "./osversion": {
      "types": "./dist/osversion.d.ts",
      "default": "./dist/osversion.js"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "default": "./dist/*/index.js"
    }
  },
  "files": [
    "dist",
    "generator",
    "bin"
  ],
  "bin": {
    "objcjs-types": "./bin/objcjs-types.ts"
  },
  "scripts": {
    "clear": "rm -rf src && rm -rf dist",
    "generate": "bun run generator/index.ts",
    "generate:custom": "bun run generator/custom.ts",
    "build": "bun run clear && bun run generate && bunx tsgo",
    "format": "prettier --write ."
  },
  "keywords": [
    "objc",
    "objective-c",
    "macos",
    "types",
    "typescript",
    "bun",
    "ffi",
    "appkit",
    "foundation"
  ],
  "license": "MIT",
  "peerDependencies": {
    "objc-js": "^1.1.0",
    "typescript": "^5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript/native-preview": "^7.0.0-dev.20260218.1",
    "objc-js": "^1.2.1",
    "prettier": "^3.8.1"
  },
  "trustedDependencies": [
    "objc-js"
  ]
}
